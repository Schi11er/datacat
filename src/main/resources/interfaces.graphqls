interface Node {
    id: ID!
}

type PageInfo {
    pageNumber: Int!
    pageSize: Int!
    pageElements: Int!
    totalPages: Int!
    hasNext: Boolean!
    hasPrevious: Boolean!
    isFirst: Boolean!
    isLast: Boolean!
}

interface XtdLanguageRepresentation {
    id: ID!
    created: String!
    createdBy: String!
    lastModified: String!
    lastModifiedBy: String!
    value: String!
    languageCode: ID!
    languageName: XtdLanguage!
}

interface XtdEntity {
    id: ID!
    created: String!
    createdBy: String!
    lastModified: String!
    lastModifiedBy: String!
    label: String!
    names: [XtdName!]!
}

interface XtdRoot {
    id: ID!
    created: String!
    createdBy: String!
    lastModified: String!
    lastModifiedBy: String!
    versionId: String!
    versionDate: String!
    label: String!
    names: [XtdName!]!
    descriptions: [XtdDescription!]!
    collectedBy: XtdRelCollectsConnection!
    associates(options: PagingOptions): XtdRelAssociatesConnection!
    associatedBy(options: PagingOptions): XtdRelAssociatesConnection!
    groups(options: PagingOptions): XtdRelGroupsConnection!
    groupedBy(options: PagingOptions): XtdRelGroupsConnection!
    specializes(options: PagingOptions): XtdRelSpecializesConnection!
    specializedBy(options: PagingOptions): XtdRelSpecializesConnection!
    composes(options: PagingOptions): XtdRelComposesConnection!
