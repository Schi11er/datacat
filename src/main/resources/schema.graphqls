    measure(id: ID!): XtdExternalDocument
    measures(term: String, options: PagingOptions): XtdMeasureWithUnitConnection!

    property(id: ID!): XtdProperty
    properties(term: String, options: PagingOptions): XtdPropertyConnection!

    subject(id: ID!): XtdSubject
    subjects(term: String, excludedIds: [String!], options: PagingOptions): XtdSubjectConnection!

    unit(id: ID!): XtdUnit
    units(term: String, options: PagingOptions): XtdUnitConnection!

    value(id: ID!): XtdValue
    values(term: String, options: PagingOptions): XtdValueConnection!

    bag(id: ID!): XtdBag
    bags(term: String, options: PagingOptions): XtdBagConnection!

    nest(id: ID!): XtdNest
    nests(term: String, options: PagingOptions): XtdNestConnection!

    documentsRelation(id: ID!): XtdRelDocuments
    documentsRelations(term: String, options: PagingOptions): XtdRelDocumentsConnection!

    associatesRelation(id: ID!): XtdRelAssociates
    associatesRelations(term: String, options: PagingOptions): XtdRelAssociatesConnection!

    groupsRelation(id: ID!): XtdRelGroups
