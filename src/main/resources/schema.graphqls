enum EntityTypes {
    XtdActivity
    XtdActor
    XtdBag
    XtdClassification
    XtdExternalDocument
    XtdMeasureWithUnit
    XtdNest
    XtdProperty
    XtdRelAssociates
    XtdRelGroups
    XtdRelSpecializes
    XtdRelComposes
    XtdSubject
    XtdUnit
    XtdValue
}

input SearchInput {
    term: String
    labels: [EntityTypes!]
    excludedLabels: [EntityTypes!]
    excludedIds: [ID!]
}

type SearchConnection {
    nodes: [XtdEntity!]!
    page: Page!
}

type Query {
    search(options: SearchInput, paging: PagingOptions): SearchConnection!

    document(id: ID!): XtdExternalDocument
    documents(term: String, options: PagingOptions): XtdExternalDocumentConnection!

    actor(id: ID!): XtdActor
